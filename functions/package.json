    "firebase-functions": "^3.14.1",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/mocha": "^9.1.0",
    "firebase-functions-test": "^0.3.3",
